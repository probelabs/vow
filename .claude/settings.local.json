{
  "permissions": {
    "allow": [
      "Edit",
      "WebSearch",
      "WebFetch(domain:typicode.github.io)",
      "Bash(git add:*)",
      "Bash(git push:*)",
      "Bash(git commit:*)",
      "Bash(node:*)",
      "Bash(npm run:*)",
      "Bash(ls:*)",
      "Bash(grep:*)",
      "Bash(chmod:*)"
    ],
    "deny": []
  },
  "enableAllProjectMcpServers": true,
  "enabledMcpjsonServers": [
    "big-brain"
  ],
  "gitCommitHook": "npx @probelabs/vow check"
}